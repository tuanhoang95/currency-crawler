ls, traders and investment bankers that benefit the entire financial market.","de":"","es":"","fr":"","it":"","pl":"","ro":"","hu":"","nl":"","pt":"","sv":"","vi":"","tr":"","ru":"","ja":"","zh":"","zh-tw":"","ko":"","ar":"","th":"","id":"","cs":"","da":"","el":"","hi":"","no":"","sk":"","uk":"","he":"","fi":"","bg":"","hr":"","lt":"","sl":""},"links":{"homepage":["https://lith.finance/"],"whitepaper":"https://docs.lith.finance/overview/what-is-lithium-finance","blockchain_site":["https://etherscan.io/token/0x188e817b02e635d482ae4d81e25dda98a97c4a42","https://platform.arkhamintelligence.com/explorer/token/lithium-finance","https://ethplorer.io/address/0x188e817b02e635d482ae4d81e25dda98a97c4a42"],"official_forum_url":["https://www.youtube.com/@lithiumfinance/featured"],"chat_url":["https://discord.gg/lithiumfinance"],"announcement_url":["https://medium.com/lithiumfinance"],"snapshot_url":null,"twitter_screen_name":"LithiumFinance","facebook_username":"","bitcointalk_thread_identifier":null,"telegram_channel_identifier":"lithiumfinanceofficial","subreddit_url":null,"repos_url":{"github":["https://github.com/LithiumFinance"],"bitbucket":[]}},"image":{"thumb":"https://coin-images.coingecko.com/coins/images/17278/thumb/Lithium_Logo-03200x200.png?1696516832","small":"https://coin-images.coingecko.com/coins/images/17278/small/Lithium_Logo-03200x200.png?1696516832","large":"https://coin-images.coingecko.com/coins/images/17278/large/Lithium_Logo-03200x200.png?1696516832"},"country_origin":"US","genesis_date":null,"contract_address":"0x188e817b02e635d482ae4d81e25dda98a97c4a42","sentiment_votes_up_percentage":0.0,"sentiment_votes_down_percentage":100.0,"watchlist_portfolio_users":7259,"market_cap_rank":7021,"market_data":{"current_price":{"aed":4.188e-05,"ars":0.01465749,"aud":1.748e-05,"bch":2.15e-08,"bdt":0.0013851,"bhd":4.3e-06,"bmd":1.14e-05,"bnb":1.4793e-08,"brl":6.362e-05,"btc":9.6367e-11,"cad":1.563e-05,"chf":9.11e-06,"clp":0.01098113,"cny":8.183e-05,"czk":0.00024066,"d