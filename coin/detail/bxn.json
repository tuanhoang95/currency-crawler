I, discounts and reduced fees on bank account and debit card.\r\n\r\nMore utility will follow in the future.\r\n","de":"","es":"","fr":"","it":"","pl":"","ro":"","hu":"","nl":"","pt":"","sv":"","vi":"","tr":"","ru":"","ja":"","zh":"","zh-tw":"","ko":"","ar":"","th":"","id":"","cs":"","da":"","el":"","hi":"","no":"","sk":"","uk":"","he":"","fi":"","bg":"","hr":"","lt":"","sl":""},"links":{"homepage":["https://blackfort.exchange/"],"whitepaper":"https://storage.googleapis.com/skydelis-static/whitepaper.pdf","blockchain_site":["https://explorer.blackfort.network/"],"official_forum_url":["https://www.linkedin.com/company/34603749","https://www.instagram.com/blackfortbxn/","https://www.tiktok.com/@blackfortofficial"],"chat_url":["https://www.youtube.com/@BlackFortOfficial"],"announcement_url":[],"snapshot_url":null,"twitter_screen_name":"blackfortbxn","facebook_username":"","bitcointalk_thread_identifier":null,"telegram_channel_identifier":"1","subreddit_url":"https://www.reddit.com","repos_url":{"github":["https://github.com/BlackFortGroup/bxn-contracts"],"bitbucket":[]}},"image":{"thumb":"https://coin-images.coingecko.com/coins/images/36544/thumb/bxn-200x200.png?1711715002","small":"https://coin-images.coingecko.com/coins/images/36544/small/bxn-200x200.png?1711715002","large":"https://coin-images.coingecko.com/coins/images/36544/large/bxn-200x200.png?1711715002"},"country_origin":"","genesis_date":null,"sentiment_votes_up_percentage":100.0,"sentiment_votes_down_percentage":0.0,"watchlist_portfolio_users":1653,"market_cap_rank":1066,"market_data":{"current_price":{"aed":0.00673793,"ars":2.36,"aud":0.00281964,"bch":3.51e-06,"bdt":0.222618,"bhd":0.00069272,"bmd":0.00183445,"bnb":2.47e-06,"brl":0.01023769,"btc":1.5544e-08,"cad":0.00251934,"chf":0.00147229,"clp":1.77,"cny":0.01316758,"czk":0.03888115,"dkk":0.01178028,"dot":0.00041759,"eos":0.00302648,"eth":5.00737e-07,"eur":0.00157753,"gbp":0.00136766,"gel":0.00497136,"hkd":0.01439538,"huf":0.629638,"idr":29.95,"ils":0.0061