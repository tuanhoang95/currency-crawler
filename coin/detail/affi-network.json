 governance utilities, empowering marketers and developers globally to take ownership of a product they actively use. Join the hive!","de":"","es":"","fr":"","it":"","pl":"","ro":"","hu":"","nl":"","pt":"","sv":"","vi":"","tr":"","ru":"","ja":"","zh":"","zh-tw":"","ko":"","ar":"","th":"","id":"","cs":"","da":"","el":"","hi":"","no":"","sk":"","uk":"","he":"","fi":"","bg":"","hr":"","lt":"","sl":""},"links":{"homepage":["https://affi.network/"],"whitepaper":"https://www.affi.network/whitepaper-2.0.pdf","blockchain_site":["https://polygonscan.com/token/0x1c15926ea330c394d891fd88f62d37ea6af953c3","https://platform.arkhamintelligence.com/explorer/token/affi-network"],"official_forum_url":["https://www.linkedin.com/company/affi-network"],"chat_url":[],"announcement_url":[],"snapshot_url":null,"twitter_screen_name":"Affi_Network","facebook_username":"","bitcointalk_thread_identifier":null,"telegram_channel_identifier":"AffiNetworkOfficial","subreddit_url":"https://www.reddit.com","repos_url":{"github":["https://github.com/orgs/affinetwork"],"bitbucket":[]}},"image":{"thumb":"https://coin-images.coingecko.com/coins/images/37116/thumb/Affi_Logo_X.png?1713330746","small":"https://coin-images.coingecko.com/coins/images/37116/small/Affi_Logo_X.png?1713330746","large":"https://coin-images.coingecko.com/coins/images/37116/large/Affi_Logo_X.png?1713330746"},"country_origin":"","genesis_date":null,"contract_address":"0x1c15926ea330c394d891fd88f62d37ea6af953c3","sentiment_votes_up_percentage":null,"sentiment_votes_down_percentage":null,"watchlist_portfolio_users":91,"market_cap_rank":7411,"market_data":{"current_price":{"aed":0.0444618,"ars":15.57,"aud":0.01860605,"bch":2.338e-05,"bdt":1.47,"bhd":0.00457108,"bmd":0.01210504,"bnb":1.64e-05,"brl":0.067556,"btc":1.02323e-07,"cad":0.01662445,"chf":0.00971526,"clp":11.66,"cny":0.086889,"czk":0.256566,"dkk":0.077735,"dot":0.00280654,"eos":0.02056627,"eth":3.37e-06,"eur":0.01040973,"gbp":0.00902485,"gel":0.03280465,"hkd":0.094991,"huf":4.